-- Alphabetizer program --

--!Start1 = -1
--!Length1 = 9
--!Start2 = -2
--!Length2 = 19

-- Init memory
    COMMENT  0 -- Allocate Lengths
    COPYFROM 23
    COPYTO   9
    COPYFROM 24
    COPYTO   19

-- Load Word 1
    COMMENT  1
loadLetter1:
    INBOX   
    JUMPZ    finishLoad1
    COPYTO   [9]
    BUMPUP   9
    JUMP     loadLetter1
finishLoad1:

-- Load Word 2
    COMMENT  2
loadLetter2:
    INBOX   
    JUMPZ    finishLoad2
    COPYTO   [19]
    BUMPUP   19
    JUMP     loadLetter2
finishLoad2:

-- Allocate "Pointers"
    COMMENT  4
    COPYFROM 23
    COPYTO   20
    COPYFROM 24
    COPYTO   21


-- Program Start
    COMMENT  3
compareLetter:
    COPYFROM [20]
    SUB      [21]
    COMMENT  5
    JUMPZ    equal
    COMMENT  9
    JUMPN    outboxWord1
    COMMENT  6
    JUMP     outboxWord2

-- Letter is same,
-- increment pointers
equal:
    COMMENT  10
    BUMPUP   20
    SUB      9
    JUMPZ    outboxWord1
    BUMPUP   21
    SUB      19
    JUMPZ    outboxWord2
    JUMP     compareLetter

-- Word 2 is higher
-- or shorter.
-- Output Word 2
outboxWord2:
    COMMENT  7
    COPYFROM [24]
    OUTBOX  
    BUMPUP   24
    SUB      19
    JUMPZ    endProgram
    JUMP     outboxWord2

-- Word 1 is higher
-- or shorter.
-- Output Word 1
outboxWord1:
    COMMENT  8
    COPYFROM [23]
    OUTBOX  
    BUMPUP   23
    SUB      9
    JUMPZ    endProgram
    JUMP     outboxWord1
endProgram:

